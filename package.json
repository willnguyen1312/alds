  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.4",
    "c8": "^8.0.1",
    "netlify-cli": "^16.3.3",
    "prettier": "^3.0.3",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  }
}
